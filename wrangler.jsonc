{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "si-cap-api",
	"main": "src/index.ts",
	"compatibility_date": "2026-01-13",
	"compatibility_flags": [
		"nodejs_compat"
	],
	// Environment variables
	"vars": {
		"ENVIRONMENT": "production"
	},
	// D1 Database
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "si-cap-db",
			"database_id": "a492de18-7cde-4ee8-b251-50729cd36bf2",
			"migrations_dir": "./migrations"
		}
	]
	// vars = { ENVIRONMENT = "staging" }
}